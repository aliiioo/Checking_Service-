
<h2>Upload File</h2>

<form asp-action="UploadFile" asp-controller="Home" enctype="multipart/form-data" method="post">
    <div>
        <label for="file">Select a file:</label>
        <input type="file" id="file" name="file" accept=".txt" />
    </div>
    <div>
        <button type="submit">Upload</button>
    </div>
</form>